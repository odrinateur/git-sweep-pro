{
  "name": "git-sweep-pro",
  "displayName": "Git Sweep Pro",
  "description": "Safely identify and prune local branches that are gone on the remote.",
  "repository": {
    "url": "git+https://github.com/MAXOUXAX/git-sweep-pro",
    "type": "git"
  },
  "publisher": "MAXOUXAX",
  "icon": "assets/icon.png",
  "version": "0.2.0",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "git-sweep-pro.run",
        "title": "Git Sweep Pro: Run"
      },
      {
        "command": "git-sweep-pro.dryRun",
        "title": "Git Sweep Pro: Dry Run"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "test:coverage": "vscode-test --coverage --coverage-output=coverage --coverage-reporter=text-summary --coverage-reporter=json-summary --coverage-reporter=lcov",
    "coverage:check": "node ./scripts/check-coverage.mjs",
    "test:ci": "npm run test:coverage && npm run coverage:check"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.54.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
